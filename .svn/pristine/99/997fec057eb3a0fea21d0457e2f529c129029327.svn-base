    <script>
        $(".lunt_xq_index_nr").each(function () {
            var a = $(this).children(".col-md-10").height();
            $(this).children(".col-md-2").height(a);
        });

        $(document).ready(function () {
            $(".qiangzhi_huifu").click(function () {
                $(".shuchu").hide();
                $(this).parent().parent().parent().parent().parent().parent().parent().parent().find(".shuchu").show();
            });


            $(".huifu").click(
                function () {
                    var nr = $(this).parent().parent().parent().find(".huifu_mr li").length;
                    var biaoji = $(this).attr("data-data");
                    if (nr == 0) {
                        if (biaoji == 0) {
                            $(".huifu").text("回复");
                            $(".huifu").attr("data-data", "0");
                            $(".shuchu").hide();
                            $(this).text("收起回复");
                            $(this).attr("data-data", "1");
                            $(this).parent().parent().find(".shuchu").show();
                        }
                        else if (biaoji == 1) {
                            $(this).text("回复");
                            $(this).attr("data-data", "0");
                            $(this).parent().parent().find(".shuchu").hide();
                        }
                        else {
                            alert(出错);
                        }
                    }
                    else {
                        var nr1 = $(this).parent().parent().parent().find(".huifu_mr").attr("data-li");
                        if (nr1 == 1) {
                            $(this).parent().parent().parent().find(".hf_nr").hide();
                            $(this).parent().parent().parent().find(".huifu_mr").attr("data-li", "0");
                            $(this).text("回复(1)");
                        }
                        else {
                            $(this).parent().parent().parent().find(".hf_nr").show();
                            $(this).parent().parent().parent().find(".huifu_mr").attr("data-li", "1");
                            $(this).text("收起回复");
                        }
                    }
                });
        })
    </script>
