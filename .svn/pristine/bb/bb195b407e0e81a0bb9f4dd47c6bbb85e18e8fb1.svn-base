    <script>
        var ul_width = $(".an_li_lunbo_top").width();//上轮播单个LI的宽度
        var li_num = $(".an_li_lunbo_top li").length;//上轮播LI的个数


        var btn_wid = $("#lb_bton_ul").width();//下轮播单个LI的宽度
        var bton_li_new = $("#lb_bton_ul li").length;
        var li_num_data = $(".an_li_lunbo_top li").length - 1;
        var li_wid = $("#lb_bton_ul li").width();//下轮播单个LI的宽度
        //    alert(parseFloat(li_wid));
        $("#lb_bton_ul").width(li_wid * bton_li_new);
        $(document).ready(function () {
            $(".an_li_lunbo_top ul").width(ul_width * li_num);
            $(".an_li_lunbo_top li").width(ul_width);
        })
        //轮播上-左
        $(".span_bian_right").click(function () {
            var num = $("#lunb_top").attr("data-data");
            if (num == li_num_data) {

                return;
            }
            else {
                //     alert("num:" + num + ",li_num:" + li_num);
                //        alert(num +":"+li_num_data)
                num++;
                $("#lunb_top").css("left", ul_width * num * -1);
            }
            $("#lunb_top").attr("data-data", num);
        })
        //轮播上-右
        $(".span_bian_left").click(function () {
            var num = $("#lunb_top").attr("data-data");
            if (num == 0) {
                return;
            }

            else {
                num--;
                $("#lunb_top").css("left", -1 * ul_width * num);
            }
            $("#lunb_top").attr("data-data", num);
        })
        //轮播下-左
        $("#lb_botton_left").click(function () {
            var new_let = parseInt($("#lb_bton_ul").css("left"));
            if (new_let == 0) {

            }
            else if (new_let * -1 < btn_wid) {
                $("#lb_bton_ul").css("left", 0);
            }
            else {
                $("#lb_bton_ul").css("left", new_let + btn_wid)
            }
        });
        //轮播下-右
        $("#lb_botton_right").click(function () {
            var new_let = parseInt($("#lb_bton_ul").css("left"));
            var zong = bton_li_new * 143;

            if (new_let == zong) {
                return;
            }
            else if ((new_let + btn_wid * -1 - btn_wid) < zong * -1) {
                $("#lb_bton_ul").css("left", zong * -1 + btn_wid)

            }
            else {
                //  alert(btn_wid);
                $("#lb_bton_ul").css("left", new_let - btn_wid);
            }
        });
        //轮播下图片点击
        $("#lb_bton_ul li").click(function () {
            var diaji = $(this).index();
            $("#lunb_top").css("left", diaji * ul_width * -1);
            $("#lunb_top").attr("data-data", diaji);
            $(this).siblings("li").attr("class", "");
            $(this).attr("class", "lunbo_li_xz");

        });
        $(".lunbo_top_b").click(function () {
            var now_bot = parseInt($("#lunb_top").attr("data-data"));
            var zuiduo = bton_li_new / 5 + 1;
            $("#lb_bton_ul li").attr("class", "");
            $("#lb_bton_ul li:eq(" + now_bot + ")").attr("class", "lunbo_li_xz");
            if (bton_li_new <= 5) {
                return;
            }
            if (now_bot < bton_li_new - 5) {
                $("#lb_bton_ul").css("left", now_bot * 143 * -1);
            }
            else {
                $("#lb_bton_ul").css("left", (bton_li_new - 5) * 143 * -1);

            }
        });

        //弹窗获得焦点
        $(".mf_yuyue_jiaod").click(function () {
            setTimeout(
                        function () {
                            $("#myModal").find("input:first").focus();
                        }, 500);
        });


    </script>
