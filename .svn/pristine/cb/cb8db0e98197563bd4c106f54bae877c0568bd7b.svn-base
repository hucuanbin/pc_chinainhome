                }

            });
            //建材类型向上滚动结束

            //建材类型向下滚动
            $("#index_neir7_jiantou_2").click(function () {
                var a = parseInt($("#jc_left_ul").attr("data-data"));
                var aa = a+ 45;
                if (a < 0) {
                    $("#jc_left_ul").attr("data-data", aa);
                    $("#jc_left_ul").css("transform", " translateY(" + aa + "px)");
                    $("#index_neir7_jiantou_1").addClass("jiancai_jiantou");
                }
                if ((a + 45) == 0) {
                    $("#index_neir7_jiantou_2").removeClass("jiancai_jiantou");
                }
            });
            //建材类型向下滚动结束
            //建材列表轮播 向右移动
            $("#jiantou4").click(function () {
                var num = parseInt((parseInt($("#myTabContent .active #index_neir7_right>div").length) / 2));
                //var aaa = parseInt(((num + 2) / 2 + 3) / 3);
                var aaaaa = num * 288;
                $("#myTabContent .active #index_neir7_right").width(aaaaa);
                var zong = $("#myTabContent .active #index_neir7_right").width();
                var zuida = zong - $(".index_neir7_2").width();
                var a = $("#myTabContent .active #index_neir7_right").attr("data-data");
                var bit_width = $("#myTabContent .active #index_neir7_right").width();
                var aa = parseInt(a) - 864;
                if (aa < -1 * zuida) {
                    aa = -1 * zuida;
                    $("#jiantou4").removeClass("jiancai_jiantou");
                }
                if (a > -1 * zuida) {
                    $("#jiantou3").addClass("jiancai_jiantou");
                    $("#myTabContent .active #index_neir7_right").attr("data-data", aa);
                    $("#myTabContent .active #index_neir7_right").css("transform", " translateX(" + aa + "px)");
                }

            });
            //建材列表轮播 向右移动结束

            //建材列表轮播 向左移动
            $("#jiantou3").click(function () {
                var num = parseInt((parseInt($("#myTabContent .active #index_neir7_right>div").length) / 2));

                //   var aaa = parseInt(((num + 2) / 2 + 3) / 3);
                $("#myTabContent .active #index_neir7_right").width(num * 288);

                var zong = $("#myTabContent .active #index_neir7_right").width();
                var zuida = zong - $(".index_neir7_2").width();

                var a = $("#myTabContent .active #index_neir7_right").attr("data-data");
                var bit_width = $("#myTabContent .active #index_neir7_right").width();

                var aa = parseInt(a) + 864;
                if (aa > 0) {
                    aa = 0;
                    $("#jiantou3").removeClass("jiancai_jiantou");
                }
                if (a < 0) {
                    $("#jiantou4").addClass("jiancai_jiantou");
                    $("#myTabContent .active #index_neir7_right").attr("data-data", aa);
                    $("#myTabContent .active #index_neir7_right").css("transform", " translateX(" + aa + "px)");
                }
            });
            //建材列表轮播 向左移动结束

            //点击建材家居类型时，加载对应家具信息
            $("#jc_left_ul li").click(function () {
                var a = 0, a11 = 0;
                $("#index_neir7_right").html("");
                $("#jc_left_ul li").attr("class", "");
                $(this).attr("class", "index_neir7_xz");
                var biaoji = $(this).children("a").attr("data-data");
                $("#index_loading").attr("style", "display:block;");
                $("#myTabContent .active #index_neir7_right").css("transform", "translateX(0px)");
                $("#index_neir7_right").css("transition", "");
                $("#index_neir7_right").attr("data-data", "0");
                $("#jiantou3").removeClass("jiancai_jiantou");
                $.ajax({
                    dataType: "json",
                    url: 'aaa2.txt?' + Math.random(),
                    success: function (data) {
                        for (var i = 0; i <= data.length; i++) {
                            if (biaoji == data[i].tid) {
                                $("#index_neir7_right").append(" <div><a href='#'> <img src=" + data[i].src + " /><p>" + data[i].neirong_p + "<span>" + data[i].neirong_sp + "</span></p></a></div>");
                            }
                            if (i == data.length - 1) {
                                $("#index_loading").attr("style", "display:none;");
                                a11 = 1;
                            }
                        }
                    }
                });
                var a4 = setInterval(
                    function () {
                        if (a11 == 1) {
                            setTimeout(
                             function () {
                                 var num = parseInt((parseInt($("#myTabContent .active #index_neir7_right>div").length) / 2));
                                 var aaaaa = num * 288;
                                 $("#myTabContent .active #index_neir7_right").width(aaaaa);
                                 $("#index_neir7_right").css("transition", "0.3s linear");
                                 if (num > 3) {
                                     $("#jiantou4").addClass("jiancai_jiantou");
                                 }
                                 else {
                                     $("#jiantou4").removeClass("jiancai_jiantou");
                                 }
                                 clearInterval(a4);
                             }, 10);
                        }
                    }, 5);

            });
            //点击建材家居类型时，加载对应家具信息结束
        });

        //初始化加载建材家居列表
        $(document).ready(function () {
            var a = 0;
            var b = 0;
            $("#index_neir7_right").html("");
            var biaoji = 1;
            $("#index_loading").attr("style", "display:block;");
            $.ajax({
                dataType: "json",
                url: 'aaa2.txt?' + Math.random(),
                success: function (data) {

                    for (var i = 0; i <= data.length; i++) {
                        if (biaoji == data[i].tid) {
                            $("#index_neir7_right").append(" <div><a href='#'> <img src=" + data[i].src + " /><p>" + data[i].neirong_p + "<span>" + data[i].neirong_sp + "</span></p></a></div>");
                            b++;
                        }
                        if (b > 7) {
                            $("#jiantou4").addClass("jiancai_jiantou");
                        }
                        $("#index_loading").attr("style", "display:none;");
                    }
                }
            });
            setTimeout(
           function () {
               var num = parseInt((parseInt($("#myTabContent .active #index_neir7_right>div").length) / 2));
               var aaaaa = num * 288;
               $("#myTabContent .active #index_neir7_right").width(aaaaa);
               $("#index_neir7_right").css("transition", "0.3s linear");
               if (num > 3) {
                   $("#jiantou4").addClass("jiancai_jiantou");
               }
               else {
                   $("#jiantou4").removeClass("jiancai_jiantou");
               }
               //   alert(aaaaa);
           }, 300);
        });
        //初始化加载建材家居完成
    </script>
